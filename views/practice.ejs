<%- include("partials/header.ejs") %>

<!-- Navbar -->
    <%- include("partials/navbar.ejs") %>

    <div class="box-one contest-div">
        <h4>Quantity matters</h4>

        <div class="row contest-row-div">
            <div class="col-3 contest-col-div">
                <span>Id</span>
            </div>
            <div class="col-6 contest-col-div">
                <span>Name</span>
            </div>
            <div class="col-3 contest-col-div contest-col-div-last">
                <span>Difficulty</span>
            </div>
        </div>

        <% for (let i=0; i<problems.length; i++){ %>
            <% if (i%2 === 0){ %> 
                <div class="row contest-row-div contest-row-div-background">
                    <div class="col-3 contest-col-div contest-col-div-info">
                        <a href="/practice/<%= problems[i].id %>"><%= problems[i].id %></a>
                    </div>
                    <div class="col-6 contest-col-div contest-col-div-info">   
                        <a href="/practice/<%= problems[i].id %>"><%= problems[i].name %></a>
                    </div>
                    <div class="col-3 contest-col-div contest-col-div-last contest-col-div-info">
                        <span><%= problems[i].difficulty %></span>
                    </div>
                </div>
            <% } else { %>
                <div class="row contest-row-div">
                    <div class="col-3 contest-col-div contest-col-div-info">
                        <a href="/practice/<%= problems[i].id %>"><%= problems[i].id %></a>
                    </div>
                    <div class="col-6 contest-col-div contest-col-div-info">
                        <a href="/practice/<%= problems[i].id %>"><%= problems[i].name %></a>
                    </div>
                    <div class="col-3 contest-col-div contest-col-div-last contest-col-div-info">
                        <span><%= problems[i].difficulty %></span>
                    </div>
                </div>
            <% } %> 
        <% }; %>
    </div>


<%- include("partials/footer.ejs") %> 